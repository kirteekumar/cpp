
#include<stdio.h>
#include <iostream>

using namespace std;

typedef int INT;

void swap (int &a, int &b)
{
}

class A 
{
	public:
		int a1;
		int a2;
		int a3;
	A(void):a1(0),a2(2),a3(0)
	{

	}
}

void input(A &obj1)
{
	cin>>obj1.a1>>obj1.a2>>obj1.a3;
}


int main()
{
	int num1 = 10;	
	int &num2 = num1;
	
	int *p1 = &num1;
	int *& p2 = p1;
	
	int arr1[3] = {10,20,30};
	int (&arr2)[3] = arr1;
		
	class A obj1;
	::input(obj1);
	
	std::cout<<obj1.a1<<obj1.a2<<obj1.a3;
	
	swap(num1,num2);

	return 0;
}


